{
  "key": "",
  "name": "getDisplayMediaZoom",
  "version": "1.0.0",
  "minimum_chrome_version": "10.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "description": "getDisplayMedia with zoom when you move your cursor in circles",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/inject.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [],
  "manifest_version": 3,
  "web_accessible_resources": [{
        "resources": ["js/*"],
        "matches": ["<all_urls>"]
      }]
}